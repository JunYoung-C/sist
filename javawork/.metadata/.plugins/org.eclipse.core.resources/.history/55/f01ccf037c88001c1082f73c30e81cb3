package day0208;

class Car {
  private String carName;
  private String carColoir;
  
  
  public Car(String carName, String carColoir) {
    super();
    this.carName = carName;
    this.carColoir = carColoir;
  }
  public String getCarName() {
    return carName;
  }
  public void setCarName(String carName) {
    this.carName = carName;
  }
  public String getCarColoir() {
    return carColoir;
  }
  public void setCarColoir(String carColoir) {
    this.carColoir = carColoir;
  }
  
  
}
public class Ex01_Inherit {
  public static void main(String[] args) {
    
  }
}
