package day0125;

public class Ex06_ForGugu {
  public static void hight_gugu() {
    for (int i = 2; i <= 9; i++) {
      System.out.println("[" + i + "ë‹¨]");
      for (int j = 1; j <= 9; j++) {
        System.out.println(i + "X" + j + "=" + (i * j));
      }
    }
  }

  public static void width_gugu() {
    for (int j = 1; j <= 9; j++) {
      for (int i = 2; i <= 4; i++) {

        System.out.print(i + "X" + j + "=" + (i * j));
      }
    }
  }

  public static void main(String[] args) {
    hight_gugu();
  }
}
