package day0127;

public class Ex09_Arrays2cha {
  public static void main(String[] args) {
    int[][] arr = {{23, 44}, {11, 22, 33}, {10, 20, 30, 40}};
    
    for (int[] rows : arr) {
      for (int col : rows) {
        System.out.print(col + " ");
      }
      System.out.println();
    }
  }
}
