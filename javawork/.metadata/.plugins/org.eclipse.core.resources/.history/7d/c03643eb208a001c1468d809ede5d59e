package day0210;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

class Shop {
  private String sang;
  private int dan;
  private int su;

  public Shop(String sang, int dan, int su) {
    super();
    this.sang = sang;
    this.dan = dan;
    this.su = su;
  }

  public String getSang() {
    return sang;
  }

  public void setSang(String sang) {
    this.sang = sang;
  }

  public int getDan() {
    return dan;
  }

  public void setDan(int dan) {
    this.dan = dan;
  }

  public int getSu() {
    return su;
  }

  public void setSu(int su) {
    this.su = su;
  }

}


