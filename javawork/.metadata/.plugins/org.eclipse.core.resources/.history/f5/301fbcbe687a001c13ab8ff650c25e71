package day0121;

import java.util.Scanner;

public class Ex06_SwitchScore {
  public static void main(String[] args) {
    Scanner stdIn = new Scanner(System.in);
    
    int score;
    System.out.println("당신의 점수는?");
    score = stdIn.nextInt();
    
    System.out.println("당신의 점수는 " + score + "점 입니다.");
  }
}
