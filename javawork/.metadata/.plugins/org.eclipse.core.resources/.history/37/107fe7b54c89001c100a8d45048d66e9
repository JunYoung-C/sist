package day0209;

public class ArrayException {
  public static void main(String[] args) {
    String[] str = {"apple", "Orange", "kiwi", "banana"};
    
    System.out.println("차례대로 출력");
    
    for (int i = 0; i <= str.length; i++) {
      System.out.println(str[i]);
      
    }
    
    for (int i = str.length; i >= 0; i--) {
      System.out.println(str[i]);
      
    }
  }
}
