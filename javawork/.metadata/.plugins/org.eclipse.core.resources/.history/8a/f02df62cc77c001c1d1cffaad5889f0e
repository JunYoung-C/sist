package day0124;

import java.util.Scanner;

public class Ex10_QuizForBreakContinue {
  public static void main(String[] args) {
   // 총 5대의 점수를 입력받아서 합계를 구한다.
    Scanner stdIn = new Scanner(System.in);
    
    int num = 0;
    int sum = 0;
    for (int i = 0; i < 5; i++) {
      num = stdIn.nextInt();
      sum += num;
    }
  }
}
