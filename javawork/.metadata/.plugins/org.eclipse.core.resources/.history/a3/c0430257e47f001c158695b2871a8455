package day0128;

public class Ex05_MemoryCount {
  
  int count = 0;
  
  
  
  public Ex05_MemoryCount() {
    count++;
    System.out.println(count);
  }



  public static void main(String[] args) {
    Ex05_MemoryCount mc1 = new Ex05_MemoryCount();
    Ex05_MemoryCount mc2 = new Ex05_MemoryCount();
  }
}
